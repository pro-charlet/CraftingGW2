<div class="bloc">
  <div class="titre"><h2>Accès à <%= link_to "votre page", member_path(0) %> ou consulter une autre page</h2></div>
  <table><tr>
  <%= form_for(Member.new) do |f| %>
    <td><%= text_field_with_auto_complete :member, :name, { :size => 35 }, { :url => members_path(:js), :method => :get, :with => "'search=' + element.value" } %></td> 
    <td><%= f.submit "Consulter", :method => 'POST' %></td>
  <% end %>
  </tr></table>
</div>
